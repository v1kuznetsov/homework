{% extends 'base.html.twig' %}

{% block title %}{{ user.name }}{% endblock %}

{% block body %}
  <div>
    <h1 class="text-center">Привет {{ user.name }}</h1>
    <h3 class="text-center">Проверь свои данные</h3>
    <p class="text-center"><img src="../images/profile.svg.png" height="100" /></p>
    <p class="text-center">Почта: {{ user.email }}</p>
    <p class="text-center">Телефон: {{ user.phone }}</p>
    <p class="text-center">Город: {{ user.city }}</p>
    <p class="text-center">Адресс: {{ user.adress }}</p>
  </div>
{% endblock %}